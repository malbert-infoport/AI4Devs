    "update-cl/common-library": "npm install @cl/common-library@latest",
    "npx:clean:cache": "npx rimraf ./.angular/cache/*",
    "ng:clean:cache": "ng cache clean",
    "postinstall": "node ./node_modules/@cl/common-library/assets/scripts/copy-resources.ts ./node_modules/@cl/common-library/assets/i18n/ ./src/assets/i18n"
